{
    "name": "rentpost/soft-deleteable-cascade",
    "description": "Provides cascade functionality to Gedmo Doctrine Extensions soft-delete. Adjusted from E-vence/SoftDeleteableListenerExtensionBundle to be framework-independent.",
    "authors": [{
        "name": "Frank Houweling",
        "homepage": "http://www.senet.nl/"
    }, {
        "name": "Ruben Harms",
        "homepage": "http://www.rubenharms.nl/"
    }],
    "keywords": ["soft-deleteable", "doctrine",  "doctrine-extensions", "gedmo"],
    "require": {
        "php": "^7.1",
        "symfony/property-access": "^4.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "FrankHouweling\\SoftDeleteableCascade\\": "src/"
        }
    }
}
